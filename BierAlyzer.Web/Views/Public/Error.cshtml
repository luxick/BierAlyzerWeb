@model BierAlyzerWeb.Models.Public.ErrorModel

@{
    ViewData["Title"] = "Management - Fehler";
}
<h4>Oh shit!</h4>

<div>Ein Fehler ist aufgetreten.</div>

@if (Model.ShowRequestId)
{
    <p>
        <strong>Request ID:</strong> <code>@Model.RequestId</code>
    </p>
}

<div class="row">
    <div class="col-12 col-sm-6 col-md-4 col-lg-3">
        <a class="btn btn-default" asp-action="Events" asp-controller="Home" style="width: 100%; color: #000;">Zurück zur Eventübersicht</a>
    </div>
</div>
